{
  "name": "@kronos-integration/service",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "Base service implementation",
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/module.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.3.6",
    "@kronos-integration/interceptor": "^10.2.3",
    "loglevel-mixin": "^6.0.1",
    "model-attributes": "^4.1.8",
    "remove-sensible-values": "^1.0.9",
    "statetransition-mixin": "^6.0.11"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}

{
  "name": "kronos-service",
  "version": "0.0.0-semantic-release",
  "description": "Base service implementation",
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "nyc npm test",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/module.js -c -- src/module.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service#readme",
  "dependencies": {
    "fast-safe-stringify": "^1.2.0",
    "kronos-endpoint": "^2.14.11",
    "loglevel-mixin": "^1.6.22",
    "model-attributes": "^3.0.10",
    "registry-mixin": "^2.1.14",
    "statetransition-mixin": "^3.1.19"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "mocha": "^3.4.2",
    "chai": "^4.0.1",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.6",
    "semantic-release": "^6.3.6",
    "nyc": "^11.0.1"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=7.10.0"
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}
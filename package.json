{
  "name": "kronos-service",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/module.js",
  "module": "src/module.js",
  "description": "Base service implementation",
  "keywords": [],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/module.js --section=API",
    "lint": "documentation lint src/module.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "fast-safe-stringify": "^2.0.2",
    "kronos-endpoint": "^2.14.15",
    "loglevel-mixin": "^3.0.0",
    "model-attributes": "^4.0.0",
    "registry-mixin": "^2.1.16",
    "statetransition-mixin": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "c8": "^3.4.0",
    "documentation": "^9.3.0",
    "esm": "^3.2.9",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.4.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-executable": "^1.4.4",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": ">=10.15.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
